    <!--覆盖第三方库的样式-->
    <!--前景色-->
    <color name="easy_photos_fg_primary">#ffffff</color> <!--前景主色，如字体颜色、icon颜色-->
    <color name="easy_photos_fg_primary_dark">#9b9b9b</color> <!--前景偏暗色，如不可用状态的字体颜色-->
    <color name="easy_photos_fg_accent">@color/orange</color> <!--前景突出色，如发送按钮的背景色，选中状态的颜色-->
    <!--背景色-->
    <color name="easy_photos_bg_primary">#000000</color> <!--背景色，如每个activity的背景色，相册的图片间隔线颜色-->
    <!--操作栏颜色-->
    <color name="easy_photos_bar_primary">@color/primary_color</color> <!--操作栏主色，如相册页顶部和底部操作栏-->
    <color name="easy_photos_bar_primary_dark">#ffffff</color> <!--操作栏偏暗色，操作栏间隔线颜色，如相册页返回按钮右边的竖线-->
    <color name="easy_photos_bar_primary_translation">@color/primary_color</color> <!--操作栏带透明的色值，如预览页的底部操作栏颜色，相册页的相机按钮背景颜色。-->
    <!--状态栏-->
    <color name="easy_photos_status_bar">@color/primary_color</color>
